{
  "version": "0.1.0",
  "description": "An example Snap written in TypeScript.",
  "proposedName": "TypeScript Example",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "gIp5YeRp7ITAZNLG+/dZtZpOuUg/Fqt06lMi6YLA1f8=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:network-access": {},
    "snap_getBip32Entropy": [
      {
        "path": ["m", "44'", "1'"],
        "curve": "secp256k1"
      }
    ],
    "snap_dialog": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "snap_manageState": {}
  },
  "manifestVersion": "0.1",
  "host_permissions": ["https://api.devnet.nil.foundation/*"]
}
